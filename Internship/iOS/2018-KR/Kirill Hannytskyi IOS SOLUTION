using namespace std;

string Eng(long long int a) {
	string str;
	switch ((a % 1000000000000000) / 100000000000000)
	{
	case 1: str += " One hundred "; break;
	case 2: str += " Two hundred "; break;
	case 3: str += " Three hundred "; break;
	case 4: str += " Four hundred "; break;
	case 5: str += " Five hundred "; break;
	case 6: str += " Six hundred "; break;
	case 7: str += " Seven hundred "; break;
	case 8: str += " Eight hundred "; break;
	case 9: str += " Nine hundred "; break;
	}
	switch ((a % 100000000000000) / 10000000000000)
	{
	case 2: str += "Twenty "; break;
	case 3: str += "Thirty "; break;
	case 4: str += "Forty "; break;
	case 5: str += "Fifty "; break;
	case 6: str += "Sixty "; break;
	case 7: str += "Seventy "; break;
	case 8: str += "Eighty "; break;
	case 9: str += "Ninety "; break;
	}
	if ((a % 100000000000000) / 1000000000000 >= 10 & (a % 100000000000000) / 1000000000000 < 20) {
		switch ((a % 100000000000000) / 1000000000000)
		{
		case 10: str += " Ten trillion "; break;
		case 11: str += " Eleven trillion "; break;
		case 12: str += " Twelve trillion "; break;
		case 13: str += " Thirdteen trillion "; break;
		case 14: str += " Fourthteen trillion "; break;
		case 15: str += " Fifthteen trillion "; break;
		case 16: str += " Sixteen trillion "; break;
		case 17: str += " Seventeen trillion "; break;
		case 18: str += " Eighteen trillion"; break;
		case 19: str += " Nineteen trillion "; break;
		}
	}
	else {
		switch ((a % 10000000000000) / 1000000000000)
		{
		case 0:	if ((a % 1000000000000000) / 1000000000000 >= 20 && (a % 1000000000000000) / 1000000000000 <= 999)
			str += " trillion "; break;
		case 1: str += " One trillion "; break;
		case 2: str += " Two trillion "; break;
		case 3: str += " Three trillion "; break;
		case 4: str += " Four trillion "; break;
		case 5: str += " Five trillion "; break;
		case 6: str += " Six trillion "; break;
		case 7: str += " Seven trillion "; break;
		case 8: str += " Eight trillion"; break;
		case 9: str += " Nine trillion "; break;
		}
	}
	switch ((a % 1000000000000) / 100000000000)
	{
	case 1: str += " One hundred "; break;
	case 2: str += " Two hundred "; break;
	case 3: str += " Three hundred "; break;
	case 4: str += " Four hundred "; break;
	case 5: str += " Five hundred "; break;
	case 6: str += " Six hundred "; break;
	case 7: str += " Seven hundred "; break;
	case 8: str += " Eight hundred "; break;
	case 9: str += " Nine hundred "; break;
	}
	switch ((a % 100000000000) / 10000000000)
	{
	case 2: str += "Twenty "; break;
	case 3: str += "Thirty "; break;
	case 4: str += "Forty "; break;
	case 5: str += "Fifty "; break;
	case 6: str += "Sixty "; break;
	case 7: str += "Seventy "; break;
	case 8: str += "Eighty "; break;
	case 9: str += "Ninety "; break;
	}
	if ((a % 100000000000) / 1000000000 >= 10 & (a % 100000000000) / 1000000000 < 20) {
		switch ((a % 100000000000) / 1000000000)
		{
		case 10: str += " Ten billion "; break;
		case 11: str += " Eleven billion "; break;
		case 12: str += " Twelve billion "; break;
		case 13: str += " Thirdteen billion "; break;
		case 14: str += " Fourthteen billion "; break;
		case 15: str += " Fifthteen billion "; break;
		case 16: str += " Sixteen billion "; break;
		case 17: str += " Seventeen billion "; break;
		case 18: str += " Eighteen billion"; break;
		case 19: str += " Nineteen billion "; break;
		}
	}
	else {
		switch ((a % 10000000000) / 1000000000)
		{
		case 0:	if ((a % 1000000000000) / 1000000000 >= 20 && (a % 1000000000000) / 1000000000 <= 999)
			str += " billion "; break;
		case 1: str += " One billion "; break;
		case 2: str += " Two billion "; break;
		case 3: str += " Three billion "; break;
		case 4: str += " Four billion "; break;
		case 5: str += " Five billion "; break;
		case 6: str += " Six billion "; break;
		case 7: str += " Seven billion "; break;
		case 8: str += " Eight billion"; break;
		case 9: str += " Nine billion "; break;
		}
	}
	switch ((a % 1000000000) / 100000000)
	{
	case 1: str += " One hundred "; break;
	case 2: str += " Two hundred "; break;
	case 3: str += " Three hundred "; break;
	case 4: str += " Four hundred "; break;
	case 5: str += " Five hundred "; break;
	case 6: str += " Six hundred "; break;
	case 7: str += " Seven hundred "; break;
	case 8: str += " Eight hundred "; break;
	case 9: str += " Nine hundred "; break;
	}
	switch ((a % 100000000) / 10000000)
	{
	case 2: str += "Twenty "; break;
	case 3: str += "Thirty "; break;
	case 4: str += "Forty "; break;
	case 5: str += "Fifty "; break;
	case 6: str += "Sixty "; break;
	case 7: str += "Seventy "; break;
	case 8: str += "Eighty "; break;
	case 9: str += "Ninety "; break;
	}
	if ((a % 100000000) / 1000000 >= 10 & (a % 100000000) / 1000000 < 20) {
		switch ((a % 100000000) / 1000000)
		{
		case 10: str += " Ten million "; break;
		case 11: str += " Eleven million "; break;
		case 12: str += " Twelve million "; break;
		case 13: str += " Thirdteen million "; break;
		case 14: str += " Fourthteen million "; break;
		case 15: str += " Fifthteen million "; break;
		case 16: str += " Sixteen million "; break;
		case 17: str += " Seventeen million "; break;
		case 18: str += " Eighteen million"; break;
		case 19: str += " Nineteen million "; break;
		}
	}
	else {

		switch ((a % 10000000) / 1000000)
		{
		case 0:	if ((a % 1000000000) / 1000000 >= 20 && (a % 1000000000) / 1000000 <= 999)
			str += " million "; break;
		case 1: str += " One million "; break;
		case 2: str += " Two million "; break;
		case 3: str += " Three million "; break;
		case 4: str += " Four million "; break;
		case 5: str += " Five million "; break;
		case 6: str += " Six million "; break;
		case 7: str += " Seven million "; break;
		case 8: str += " Eight million"; break;
		case 9: str += " Nine million "; break;
		}
	}
	switch ((a % 1000000) / 100000)
	{
	case 1: str += " One hundred "; break;
	case 2: str += " Two hundred "; break;
	case 3: str += " Three hundred "; break;
	case 4: str += " Four hundred "; break;
	case 5: str += " Five hundred "; break;
	case 6: str += " Six hundred "; break;
	case 7: str += " Seven hundred "; break;
	case 8: str += " Eight hundred "; break;
	case 9: str += " Nine hundred "; break;
	}
	switch ((a % 100000) / 10000)
	{
	case 2: str += "Twenty "; break;
	case 3: str += "Thirty "; break;
	case 4: str += "Forty "; break;
	case 5: str += "Fifty "; break;
	case 6: str += "Sixty "; break;
	case 7: str += "Seventy "; break;
	case 8: str += "Eighty "; break;
	case 9: str += "Ninety "; break;
	}
	if ((a % 100000) / 1000 >= 10 & (a % 100000) / 1000 < 20) {
		switch ((a % 100000) / 1000)
		{
		case 10: str += " Ten thousand "; break;
		case 11: str += " Eleven thousand "; break;
		case 12: str += " Twelve thousand "; break;
		case 13: str += " Thirdteen thousand "; break;
		case 14: str += " Fourthteen thousand "; break;
		case 15: str += " Fifthteen thousand "; break;
		case 16: str += " Sixteen thousand "; break;
		case 17: str += " Seventeen thousand "; break;
		case 18: str += " Eighteen thousand"; break;
		case 19: str += " Nineteen thousand "; break;
		}
	}
	else {
		switch ((a % 10000) / 1000)
		{
		case 0:if ((a % 1000000) / 1000 >= 20 && (a % 1000000) / 1000 <= 999)
			str += " thousand "; break;
		case 1: str += " One thousand "; break;
		case 2: str += " Two thousand "; break;
		case 3: str += " Three thousand "; break;
		case 4: str += " Four thousand "; break;
		case 5: str += " Five thousand "; break;
		case 6: str += " Six thousand "; break;
		case 7: str += " Seven thousand "; break;
		case 8: str += " Eight thousand"; break;
		case 9: str += " Nine thousand "; break;
		}
	}
	switch ((a % 1000) / 100)
	{
	case 1: str += " One hundred "; break;
	case 2: str += " Two hundred "; break;
	case 3: str += " Three hundred "; break;
	case 4: str += " Four hundred "; break;
	case 5: str += " Five hundred "; break;
	case 6: str += " Six hundred "; break;
	case 7: str += " Seven hundred "; break;
	case 8: str += " Eight hundred "; break;
	case 9: str += " Nine hundred "; break;
	}switch ((a % 100) / 10)
	{
	case 2: str += "Twenty "; break;
	case 3: str += "Thirty "; break;
	case 4: str += "Forty"; break;
	case 5: str += "Fifty "; break;
	case 6: str += "Sixty"; break;
	case 7: str += "Seventy"; break;
	case 8: str += "Eighty "; break;
	case 9: str += "Ninety "; break;
	}
	if (a % 100 >= 10 & a % 100 < 20) {
		switch (a % 100) {
		case 10: str += "Ten"; break;
		case 11: str += "Eleven"; break;
		case 12: str += "Twelve"; break;
		case 13: str += "Thirdteen"; break;
		case 14: str += "Fourthteen"; break;
		case 15: str += "Fifthteen"; break;
		case 16: str += "Sixteen"; break;
		case 17: str += "Seventeen"; break;
		case 18: str += "Eighteen"; break;
		case 19: str += "Nineteen"; break;
		}
	}
	else {
		switch (a % 10)
		{
		case 1: str += " One\n"; break;
		case 2: str += " Two\n"; break;
		case 3: str += " Three\n"; break;
		case 4: str += " Fouth\n"; break;
		case 5: str += " Five\n"; break;
		case 6: str += " Six\n"; break;
		case 7: str += " Seven\n"; break;
		case 8: str += " Eight\n"; break;
		case 9: str += " Nine\n"; break;
		}
	}
	return str;
	_getch();
}

string D(long long int a) {
	string str;
	switch ((a % 1000000000000000) / 100000000000000)
	{
	case 1: str += " Hundert "; break;
	case 2: str += " Zweihundert "; break;
	case 3: str +=  " Dreihundert "; break;
	case 4: str += " Vierhundert "; break;
