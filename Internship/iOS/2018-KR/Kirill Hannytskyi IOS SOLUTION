		case 15: str +=  " Fifthteentausend "; break;
		case 16: str +=  " Sixteentausend "; break;
		case 17: str += " Seventeentausend "; break;
		case 18: str += " Eighteentausend "; break;
		case 19: str += " Nineteentausend "; break;
		}
	}
