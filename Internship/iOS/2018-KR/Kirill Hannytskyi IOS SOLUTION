	case 8: str +=  " Achthundert "; break;
	case 9: str += " Neunhundert "; break;
	}switch ((a % 100) / 10)
	{
	case 2: str += "Zwanzig "; break;
	case 3: str += "Dreißig "; break;
	case 4: str += "Vierzig"; break;
	case 5: str += "Fünfzig "; break;
	case 6: str += "Sechzig"; break;
	case 7: str +=  "Siebzig"; break;
	case 8: str += "Achtzig "; break;
	case 9: str += "Neunzig "; break;
	}
	if (a % 100 >= 10 & a % 100 < 20) {
		switch (a % 100) {
		case 10: str +=  "Zehn"; break;
