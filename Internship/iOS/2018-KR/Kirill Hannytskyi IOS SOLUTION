		case 15: str += "Fünfzehn"; break;
		case 16: str += "Sechzehn"; break;
		case 17: str += "Siebzehn"; break;
		case 18: str += "Achtzehn"; break;
		case 19: str += "Neunzehn"; break;
		}
	}
	else {
		switch (a % 10)
		{
		case 1: str += " Eins\n"; break;
		case 2: str += " Zwei\n"; break;
		case 3: str += " Drei\n"; break;
		case 4: str +=  " Vier\n"; break;
		case 5: str += " Fünf\n"; break;
		case 6: str += " Sechs\n"; break;
		case 7: str += " Sieben\n"; break;
		case 8: str += " Acht\n"; break;
		case 9: str += " Neun\n"; break;
		}
	}
	return str;
	_getch();
}

string Uah(long long int a) {
	string str;
	switch ((a % 1000000000000000) / 100000000000000)
	{
	case 1: str += " Сто "; break;
	case 2: str += " Двісті "; break;
	case 3: str +=  " Триста "; break;
	case 4: str += " Чотириста "; break;
	case 5: str += " П'ятьсот "; break;
	case 6: str += " Шістьсот "; break;
	case 7: str += " Сімсот "; break;
	case 8: str += " Вісімсот "; break;
	case 9: str += " Дев'ятьсот "; break;
	}
	switch ((a % 100000000000000) / 10000000000000)
	{
	case 2: str += "Двадцять "; break;
	case 3: str += "Тридцять "; break;
	case 4: str += "Сорок"; break;
	case 5: str +=  "П'ятдесят "; break;
	case 6: str += "Шістдесят"; break;
	case 7: str += "Сімдесят"; break;
	case 8: str += "Вісімдесять "; break;
	case 9: str += "Дев'яносто "; break;
	}
	if ((a % 100000000000000) / 1000000000000 >= 10 & (a % 100000000000000) / 1000000000000 < 20) {
		switch ((a % 100000000000000) / 1000000000000)
		{
		case 10: str += " Десять трильйонів "; break;
		case 11: str += " Одинадцять трильйонів "; break;
		case 12: str += " Дванадцять трильйонів "; break;
		case 13: str += " Тринадцять трильйонів "; break;
		case 14: str += " Чотирнадцять трильйонів "; break;
		case 15: str += " П'ятнадцять трильйонів "; break;
		case 16: str +=  " Шістнадцять трильйонів "; break;
		case 17: str += " Сімнадцять трильйонів "; break;
		case 18: str += " Вісімнадцять трильйонів"; break;
		case 19: str += " Дев'ятнадцять трильйонів "; break;
		}
	}
	else {
		switch ((a % 10000000000000) / 1000000000000)
		{
		case 0:	if ((a % 1000000000000000) / 1000000000000 >= 20 && (a % 1000000000000000) / 1000000000000 <= 999)
			str += " трильйонів"; break;
		case 1: str += " Один трильйон"; break;
		case 2: str += " Два трильйони"; break;
		case 3: str += " Три трильйони"; break;
		case 4: str += " Чотири трильйони"; break;
		case 5: str += " П'ять трильйонів"; break;
		case 6: str +=  " Шість трильйонів"; break;
		case 7: str += " Сім трильйонів"; break;
		case 8: str += " Вісім трильйонів"; break;
		case 9: str += "Дев'ять трильйонів"; break;
		}
	}
	switch ((a % 1000000000000) / 100000000000)
	{
	case 1: str += " Сто "; break;
	case 2: str += " Двісті "; break;
	case 3: str += " Триста "; break;
	case 4: str += " Чотириста "; break;
	case 5: str += " П'ятьсот "; break;
	case 6: str += " Шістьсот "; break;
	case 7: str += " Сімсот "; break;
	case 8: str += " Вісімсот "; break;
	case 9: str += " Дев'ятьсот "; break;
	}
	switch ((a % 100000000000) / 10000000000)
	{
	case 2: str += "Двадцять "; break;
	case 3: str += "Тридцять "; break;
	case 4: str += "Сорок"; break;
	case 5: str += "П'ятдесят "; break;
	case 6: str += "Шістдесят"; break;
	case 7: str += "Сімдесят"; break;
	case 8: str += "Вісімдесять "; break;
	case 9: str += "Дев'яносто "; break;
	}
	if ((a % 100000000000) / 1000000000 >= 10 & (a % 100000000000) / 1000000000 < 20) {
		switch ((a % 100000000000) / 1000000000)
		{
		case 10: str += " Десять мiльярдів "; break;
		case 11: str += " Одинадцять мiльярдів "; break;
		case 12: str += " Дванадцять мiльярдів "; break;
		case 13: str += " Тринадцять мiльярдів "; break;
		case 14: str += " Чотирнадцять мiльярдів "; break;
		case 15: str += " П'ятнадцять мiльярдів "; break;
		case 16: str += " Шістнадцять мiльярдів "; break;
		case 17: str += " Сімнадцять мiльярдів "; break;
		case 18: str += " Вісімнадцять мiльярдів"; break;
		case 19: str += " Дев'ятнадцять мiльярдів "; break;
		}
	}
	else {
		switch ((a % 10000000000) / 1000000000)
		{
		case 0:	if ((a % 1000000000000) / 1000000000 >= 20 && (a % 1000000000000) / 1000000000 <= 999)
			str += " мiльярдів "; break;
		case 1: str += " Один мiльярд"; break;
		case 2: str += " Два мiльярди"; break;
		case 3: str += " Три мiльярди"; break;
		case 4: str += " Чотир мiльярди"; break;
		case 5: str += " П'ять мiльярдів"; break;
		case 6: str += " Шість мiльярдів"; break;
		case 7: str += " Сім мiльярдів"; break;
		case 8: str += " Вісім мiльярдів"; break;
		case 9: str += "Дев'ять мiльярдів"; break;
		}
	}
	switch ((a % 1000000000) / 100000000)
	{
	case 1: str += " Сто "; break;
	case 2: str += " Двісті "; break;
	case 3: str += " Триста "; break;
	case 4: str += " Чотириста "; break;
	case 5: str += " П'ятьсот "; break;
	case 6: str += " Шістьсот "; break;
	case 7: str += " Сімсот "; break;
	case 8: str += " Вісімсот "; break;
	case 9: str += " Дев'ятьсот "; break;
	}
	switch ((a % 100000000) / 10000000)
	{
	case 2: str += "Двадцять "; break;
	case 3: str += "Тридцять "; break;
	case 4: str += "Сорок"; break;
	case 5: str += "П'ятдесят "; break;
	case 6: str += "Шістдесят"; break;
	case 7: str += "Сімдесят"; break;
	case 8: str += "Вісімдесять "; break;
	case 9: str += "Дев'яносто "; break;
	}
	if ((a % 100000000) / 1000000 >= 10 & (a % 100000000) / 1000000 < 20) {
		switch ((a % 100000000) / 1000000)
		{
		case 10: str += " Десять мiльйонів "; break;
		case 11: str += " Одинадцять мiльйонів "; break;
		case 12: str += " Дванадцять мiльйонів "; break;
		case 13: str += " Тринадцять мiльйонів "; break;
		case 14: str += " Чотирнадцять мiльйонів "; break;
		case 15: str += " П'ятнадцять мiльйонів "; break;
		case 16: str += " Шістнадцять мiльйонів "; break;
		case 17: str += " Сімнадцять мiльйонів "; break;
		case 18: str += " Вісімнадцять мiльйонів"; break;
		case 19: str += " Дев'ятнадцять мiльйонів "; break;
		}
	}
	else {

		switch ((a % 10000000) / 1000000)
		{
		case 0:	if ((a % 1000000000) / 1000000 >= 20 && (a % 1000000000) / 1000000 <= 999)
			str += " мiльйонів "; break;
		case 1: str += " Один мiльйон"; break;
		case 2: str += " Два мiльйони"; break;
		case 3: str += " Три мiльйони"; break;
		case 4: str += " Чотир мiльйони"; break;
		case 5: str += " П'ять мiльйонів"; break;
		case 6: str += " Шість мiльйонів"; break;
		case 7: str += " Сім мiльйонів"; break;
		case 8: str += " Вісім мiльйонів"; break;
		case 9: str += "Дев'ять мiльйонів"; break;
		}
	}
	switch ((a % 1000000) / 100000)
	{
	case 1: str += " Сто "; break;
	case 2: str += " Двісті "; break;
	case 3: str += " Триста "; break;
	case 4: str += " Чотириста "; break;
	case 5: str += " П'ятьсот "; break;
	case 6: str += " Шістьсот "; break;
	case 7: str += " Сімсот "; break;
	case 8: str += " Вісімсот "; break;
	case 9: str += " Дев'ятьсот "; break;
	}
	switch ((a % 100000) / 10000)
	{
	case 2: str += "Двадцять "; break;
	case 3: str += "Тридцять "; break;
	case 4: str += "Сорок"; break;
	case 5: str += "П'ятдесят "; break;
	case 6: str += "Шістдесят"; break;
	case 7: str += "Сімдесят"; break;
	case 8: str += "Вісімдесять "; break;
	case 9: str += "Дев'яносто "; break;
	}
	if ((a % 100000) / 1000 >= 10 & (a % 100000) / 1000 < 20) {
		switch ((a % 100000) / 1000)
		{
		case 10: str += " Десять тисяч "; break;
		case 11: str += " Одинадцять тисяч "; break;
		case 12: str += " Дванадцять тисяч "; break;
		case 13: str += " Тринадцять тисяч "; break;
		case 14: str += " Чотирнадцять тисяч "; break;
		case 15: str += " П'ятнадцять тисяч "; break;
		case 16: str += " Шістнадцять тисяч "; break;
		case 17: str += " Сімнадцять тисяч "; break;
		case 18: str += " Вісімнадцять тисяч"; break;
		case 19: str += " Дев'ятнадцять тисяч "; break;
		}
	}
	else {
		switch ((a % 10000) / 1000)
		{
		case 0:if ((a % 1000000) / 1000 >= 20 && (a % 1000000) / 1000 <= 999)
			str += " тисяч "; break;
		case 1: str += " Одна тисяча "; break;
		case 2: str += " Дві тисячі "; break;
		case 3: str += " Три тисячі "; break;
		case 4: str += " Чотири тисячі "; break;
		case 5: str += " П'ять тисяч "; break;
		case 6: str += " Шість тисячі "; break;
		case 7: str += " Сім тисяч "; break;
		case 8: str += " Вісім тисяч"; break;
		case 9: str += " Дев'ять тисяч "; break;
		}
	}
	switch ((a % 1000) / 100)
	{
	case 1: str+=" Сто "; break;
	case 2: str += " Двісті "; break;
	case 3: str +=  " Триста "; break;
	case 4: str += " Чотириста "; break;
	case 5: str += " П'ятьсот "; break;
	case 6: str += " Шістьсот "; break;
	case 7: str += " Сімсот "; break;
	case 8: str += " Вісімсот "; break;
	case 9: str += " Дев'ятьсот "; break;
	}switch ((a % 100) / 10)
	{
	case 2: str += "Двадцять "; break;
	case 3: str += "Тридцять "; break;
	case 4: str += "Сорок"; break;
	case 5: str += "П'ятдесят "; break;
	case 6: str += "Шістдесят"; break;
	case 7: str += "Сімдесят"; break;
	case 8: str += "Вісімдесять "; break;
	case 9: str += "Дев'яносто "; break;
	}
	if (a % 100 >= 10 & a % 100 < 20) {
		switch (a % 100) {
		case 10: str += "Десять"; break;
		case 11: str += "Одинадцять"; break;
		case 12: str += "Дванадцять"; break;
		case 13: str += "Тринадцять"; break;
		case 14: str += "Чотирнадцять"; break;
		case 15: str += "П'ятнадцять"; break;
		case 16: str += "Шістнадцять"; break;
		case 17: str += "Сімнадцять"; break;
		case 18: str += "Вісімнадцять"; break;
		case 19: str += "Дев'ятнадцять"; break;
		}
	}
	else {
		switch (a % 10)
		{
		case 1: str += " Один\n"; break;
		case 2: str += " Два\n"; break;
		case 3: str += " Три\n"; break;
		case 4: str += " Чотири\n"; break;
		case 5: str += " П'ять\n"; break;
		case 6: str += " Шість\n"; break;
		case 7: str += " Сім\n"; break;
		case 8: str += " Вісім\n"; break;
		case 9: str += "Девтриллион'ять\n" ; break;
		}
	}
	return str;
	_getch();
}



void main() {
	SetConsoleCP(1251);
	SetConsoleOutputCP(1251);
	long long int a;
	int choice;
	cout << "1.ENGLISH,2.UKRAINIAN,3.GERMAN\n";
	cin >> choice;
	switch (choice)
	{
	case 1: {
		cout << "Введите число: ";
		cin >> a;
		if (a < 0) {
			cout << "Минусовое число";

		}
		else if (a >= 1000000000000000) {
			cout << "Вы ввели >= квадраллион";
		}
		cout << "Ваше число: " << " ";
		string str = Eng(a);
		cout << str;
		system("pause");
	}break;
	case 2: {
		cout << "Введите число: ";
		cin >> a;
		if (a < 0) {
			cout << "Минусовое число";

		}
		else if (a >= 1000000000000000) {
			cout << "Вы ввели >= квадраллион";
		}
		cout << "Ваше число: " << " ";
		string str = Uah(a);
		cout << str;
		system("pause");
	}break;
	case 3: {
		cout << "Введите число: ";
		cin >> a;
		if (a < 0) {
			cout << "Минусовое число";

		}
		else if (a >= 1000000000000000) {
			cout << "Вы ввели >= квадраллион";
		}
		cout << "Ваше число: " << " ";
		string str = D(a);
		cout << str;
		system("pause");
	}break;
	default:
		break;
	}

}
